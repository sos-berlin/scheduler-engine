<xml_element name          = "param"
             title         = "Parameter"
             category      = "Antwort"
             sub_directory = "answer/"
             base_dir      = "../../"
             parent_page   = "../../xml_commands.xml"
             author        = "$Author$"
             date          = "$Date$">

    <xml_parent_elements>
        <xml_parent_element name="answer"/>
    </xml_parent_elements>

    <xml_attributes>
        <xml_attribute name="name" type="name" title="Name des Parameters"/>
        <xml_attribute name="value" type="value" title="Wert des Parameters"/>
    </xml_attributes>

</xml_element>
