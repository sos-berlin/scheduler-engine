<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet href="../../scheduler.xsl" type="text/xsl"?>

<!--$Id: queued_task.xml,v 1.2 2004/09/07 06:25:37 jz Exp $-->

<!--         title         = "Task in der Warteschlange"-->

<xml_element name          = "queued_task" 
             title         = "Tasks in the Task Queue"
             category      = "Answer"
             sub_directory = "answer/"
             base_dir      = "../../"
             parent_page   = "../../xml_commands.xml"
             author        = "$Author: jz $"
             date          = "$Date$">

    <xml_parent_elements>
        <xml_parent_element name="queued_task"/>
    </xml_parent_elements>
   
    
    <xml_attributes>
        <xml_attribute name="task" type="number" title="Task Id"/>
        <xml_attribute name="enqueued" type="yyyy-mm-dd hh:mm:ss.mmm" title="Time of entry in the task queue"/>
        <xml_attribute name="start_at" type="yyyy-mm-dd hh:mm:ss.mmm" title="Task start time"/>
        <xml_attribute name="name" type="text" title="Task Name"/>
    <!--<xml_attribute name="task" type="zahl" title="Task-Id"/>
        <xml_attribute name="enqueued" type="yyyy-mm-dd hh:mm:ss.mmm" title="Zeitpunkt der Aufnahme in die Warteschlange"/>
        <xml_attribute name="start_at" type="yyyy-mm-dd hh:mm:ss.mmm" title="Wann die Task starten soll"/>
        <xml_attribute name="name" type="text" title="Name der Task"/>-->
    </xml_attributes>
    
    <xml_child_elements>
        <xml_child_element name="params" directory="xml/"/>
    </xml_child_elements>

    <description>
        <p>
        </p>
    </description>
    
</xml_element>
